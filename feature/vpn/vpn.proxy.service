[Unit]
Description=VPN service through hiddify
After=network.target

[Service]
Type=simple
WorkingDirectory=/home/walpy/hub/feature/vpn
ExecStart=/home/walpy/hub/feature/vpn/start.proxy
ExecStopPost=/home/walpy/hub/feature/vpn/stop.proxy
SyslogIdentifier="vpn-proxy"
Restart=always
RestartSec=3

[Install]
WantedBy=default.target
