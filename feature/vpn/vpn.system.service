[Unit]
Description=VPN service through hiddify
After=network.target

[Service]
Type=simple
WorkingDirectory=/home/walpy/hub/feature/vpn
ExecStart=/home/walpy/hub/feature/vpn/start.system
ExecStopPost=/home/walpy/hub/feature/vpn/stop.system
SyslogIdentifier="vpn-system"
Restart=always
RestartSec=3

[Install]
WantedBy=default.target
