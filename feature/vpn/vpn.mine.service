[Unit]
Description=VPN service through hiddify
After=network.target

[Service]
Type=simple
WorkingDirectory=/home/walpy/hub/feature/vpn
ExecStart=/home/walpy/hub/feature/vpn/start
ExecStopPost=/home/walpy/hub/feature/vpn/stop
SyslogIdentifier="vpn-mine"
Restart=always
RestartSec=3

[Install]
WantedBy=default.target
